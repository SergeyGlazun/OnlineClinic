@model OnlineClinic.Models.ExportStatictic


<div class="container">
    <div class="row">
        <div class="col-lg-6">
            <h5>Статистика</h5>
            <ul>
                <li>Количество зарегистрированных пользователей: @Model.CountUsers</li>
                <li>Количество сотрудников: @Model.CountDoctors</li>
                <li>Общее количество записей: @Model.CountRequest</li>
            </ul>
        </div>
        <div class="col-lg-6">
            <h5>Зарегистрировать нового сотрудника</h5>
            @await Html.PartialAsync("RegistrationNewDoctor", @Model.model)

        </div>
    </div>
</div>